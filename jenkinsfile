pipeline {
    agent any

    tools {
        jdk 'JDK17'
        maven 'maven3'
    }

    stages {

        stage('GIT') {
            steps {
                git branch: 'master',
                    url: 'https://github.com/hmemali/hmem_ali_4TWIN8.git'
            }
        }

        stage('Compile Stage') {
            steps {
                sh 'mvn clean compile'
            }
        }
    }
}

